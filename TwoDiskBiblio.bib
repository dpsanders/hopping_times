@article{three_hard_discs_2004,
title = "Exact statistics of three-hard-disk system in two-dimensional space",
journal = "Physica A: Statistical Mechanics and its Applications",
volume = "334",
number = "1",
pages = "187 - 200",
year = "2004",
issn = "0378-4371",
doi = "https://doi.org/10.1016/j.physa.2003.11.004",
url = "http://www.sciencedirect.com/science/article/pii/S037843710301094X",
author = "Zhoujian Cao and Haihong Li and Toyonori Munakata and Dahai He and Gang Hu",
keywords = "Ergodic, Van der Waals",
abstract = "A system with three hard particles in two-dimensional rectangle container is investigated. The system motion is chaotic and ergodic in energy surface. The partition function of the system in its energy surface is analytically calculated based on a suitable configuration classification. Theoretical results are fully confirmed by numerical simulations. A van der Waals-like behavior, i.e., increasing system size results in increasing pressure on the container wall, is revealed in some region of the container size. This generalizes the observation of the two-hard-disk system of Phys. Rev. E 65 (2002) 066104."
}

